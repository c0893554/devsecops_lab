    <script>pipeline {
    agent any
    environment {
        SONARQUBE_SERVER = &apos;SonarQube&apos;
    }
    stages {
        stage(&apos;Checkout Code&apos;) {
            steps {
                sshagent([&apos;SSH-Key&apos;]) {
                    git branch: &apos;master&apos;, url: &apos;https://github.com/c0893554/devsecops_lab&apos;
            
                }
            }
        }
        
        stage(&apos;Set up WordPress Stage Environment&apos;) {
            steps {
                script {
                    sh &apos;cd Website/compose/stage/&apos;
                    sh &apos;docker-compose up -d&apos;
                }
            }
        }

        stage(&apos;Run SonarQube Scan&apos;) {
            steps {
                withSonarQubeEnv(SONARQUBE_SERVER) {
                    script {
                        sh &apos;&apos;&apos;
                        sonar-scanner \
                          -Dsonar.projectKey=wordpress-site \
                          -Dsonar.sources=./wordpress \
                          -Dsonar.host.url=$SONAR_HOST_URL \
                          -Dsonar.login=$SONAR_AUTH_TOKEN
                        &apos;&apos;&apos;
                    }
                }
            }
        }

        stage(&apos;Quality Gate&apos;) {
            steps {
                script {
                    timeout(time: 1, unit: &apos;HOURS&apos;) {
                        waitForQualityGate abortPipeline: true
                    }
                }
            }
        }

        stage(&apos;Clean up&apos;) {
            steps {
                script {
                    sh &apos;docker-compose -f docker-compose.stage.yml down&apos;
                }
            }
        }
    }

    post {
        always {
            sh &apos;docker-compose -f docker-compose.stage.yml down&apos;
        }
    }
}
